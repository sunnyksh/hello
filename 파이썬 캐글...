import numpy as np
import pandas as pd

train=pd.read_csv('/.../.../파일이름.csv')

train.head()

def greet(first_name, last_name):
    greeting = 'My name is ' + last_name + ', ' + first_name + ' ' + last_name + '!'
    return greeting

# Replace with your first and last name.
# That is, unless your name is already James Bond.
greet('James', 'Bond')
